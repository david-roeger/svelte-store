{
  "name": "@tanstack/svelte-store",
  "version": "0.2.0",
  "author": "Tanner Linsley",
  "license": "MIT",
  "repository": "tanstack/react-store",
  "homepage": "https://tanstack.com/",
  "description": "",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/tannerlinsley"
  },
  "type": "module",
  "types": "build/index.d.ts",
  "module": "build/index.svelte.js",
  "svelte": "./build/index.svelte.js",
  "exports": {
    ".": {
        "types": "./build/index.d.ts",
        "svelte": "./build/index.svelte.js",
        "import": "./build/index.svelte.js",
        "default": "./build/index.svelte.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf ./build && rimraf ./coverage",
    "test:types": "svelte-check --tsconfig ./tsconfig.json",
    "test:eslint": "eslint --ext .svelte,.ts ./src",
    "test:lib": "vitest run --coverage",
    "test:lib:dev": "pnpm run test:lib --watch",
    "test:build": "publint --strict",
    "build": "svelte-package --input ./src --output ./build"
  },
  "nx": {
    "targets": {
      "test:lib": {
        "dependsOn": [
          "test:types"
        ]
      }
    }
  },
  "files": [
    "build",
    "src"
  ],
  "dependencies": {
    "@tanstack/store": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.3.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@testing-library/svelte": "^5.1.0",
    "eslint-plugin-svelte": "^2.39.0",
    "svelte": "^5.0.0-next.138",
    "svelte-check": "^3.7.1"
  },
  "peerDependencies": {},
  "peerDependenciesMeta": {
    "svelte": ">=5 <6"
  }
}
